{
  "name": "template-mail-webf",
  "version": "1.0.0",
  "description": "Template Mail VueJS",
  "author": "Tribu WebF",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest --coverage"
  },
  "dependencies": {
    "nuxt": "^1.0.0",
    "vue": "^2.5.16",
    "vue-server-renderer": "^2.5.16"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "vue",
      "json",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.0.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^23.1.0",
    "jest-vue-preprocessor": "^1.4.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "vue-markdown-loader": "^2.4.0",
    "vue-test-utils": "^1.0.0-beta.11"
  }
}
